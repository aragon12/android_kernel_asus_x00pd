		qcom,cam-vreg-op-mode = <1200000 1800000 2800000>;
		qcom,gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		//chenjiehua@wind-mobi.com change for cam-vdig start
		pinctrl-0 = <&cam_sensor_mclk1_default &cam_sensor_front_default &cam_sensor_mipi_switch_default &cam_sensor_rear_vdig>;
		pinctrl-1 = <&cam_sensor_mclk1_sleep &cam_sensor_front_sleep &cam_sensor_mipi_switch_sleep &cam_sensor_rear_vdig_sleep>;
		gpios = <&tlmm 27 0>,
			<&tlmm 36 0>,
			<&tlmm 50 0>,
			<&tlmm 97 0>,
			<&tlmm 96 0>,
			<&tlmm 99 0>;
		qcom,gpio-reset = <1>;
		qcom,gpio-standby = <2>;
		qcom,gpio-custom1 = <3>;
		qcom,gpio-custom2 = <4>;
		qcom,gpio-vdig = <5>;
		qcom,gpio-req-tbl-num = <0 1 2 3 4 5>;
		qcom,gpio-req-tbl-flags = <1 0 0 0 0 0>;
                qcom,gpio-req-tbl-label = "CAMIF_MCLK1",
					  "CAM_RESET1",
					  "CAM_STANDBY1",
					  "CAM_MIPIEN",
			          "CAM_MIPISEL",
					  "CAM_VDIG";
		//chenjiehua@wind-mobi.com change for cam-vdig end
