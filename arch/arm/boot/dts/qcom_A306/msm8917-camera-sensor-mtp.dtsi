		qcom,cam-vreg-op-mode = <1200000 1800000 2800000>;
		pinctrl-names = "cam_default", "cam_suspend";
		//chenjiehua@wind-mobi.com change for cam-vdig start
		pinctrl-0 = <&cam_sensor_mclk2_default &cam_sensor_front1_default &cam_sensor_mipi_switch_default &cam_sensor_rear_vdig>;
		pinctrl-1 = <&cam_sensor_mclk2_sleep &cam_sensor_front1_sleep &cam_sensor_mipi_switch_sleep &cam_sensor_rear_vdig_sleep>;
		gpios = <&tlmm 28 0>,
