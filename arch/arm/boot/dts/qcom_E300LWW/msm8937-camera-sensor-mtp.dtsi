		qcom,cam-vreg-op-mode = <1200000  2800000>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk2_default &cam_sensor_front1_default &cam_sensor_mipi_switch_default &cam_sensor_vio>;
		pinctrl-1 = <&cam_sensor_mclk2_sleep &cam_sensor_front1_sleep &cam_sensor_mipi_switch_sleep &cam_sensor_vio_sleep>;
		gpios = <&tlmm 28 0>,
